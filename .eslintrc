{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "prefer-arrow-callback": "off",
    "no-console": "off",
    "object-curly-spacing": "off",
    "quotes": ["error", "double"],
    "func-names": "off",
    "arrow-parens": [2, "as-needed"],
    "linebreak-style": "off",
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb/base",
    "eslint:recommended"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
